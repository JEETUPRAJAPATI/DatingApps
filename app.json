{
  "expo": {
    "name": "Heart2get",
    "slug": "heart2get",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "heart2get",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2b6fdab1-1778-4643-897a-4fc48ba88cd8"
      }
    },
    "owner": "jection",
    "android": {
      "package": "com.jection.heart2get"
    }
  }
}